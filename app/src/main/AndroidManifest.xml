<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.homepage">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.HomePage">
        <activity android:name=".TicketInformation"></activity>
        <activity
            android:name=".PassengerDetailsActivity"
            android:parentActivityName=".FlightActivity" />
        <activity
            android:name=".ContactWithEmailActivity"
            android:parentActivityName=".ProfileActivity" />
        <activity
            android:name=".CardFormActivity"
            android:parentActivityName=".PassengerDetailsActivity" />
        <activity
            android:name=".CustomerServiceActivity"
            android:parentActivityName=".ProfileActivity" />
        <activity android:name=".CurrencyActivity" />
        <activity
            android:name=".TermsOfServiceActivity"
            android:parentActivityName=".SettingsActivity" />
        <activity
            android:name=".PrivacyPolicyActivity"
            android:parentActivityName=".SettingsActivity" />
        <activity
            android:name=".SettingsActivity"
            android:parentActivityName=".ProfileActivity" />
        <activity
            android:name=".FlightActivity"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".ResetPasswordActivity"
            android:parentActivityName=".ProfileActivity" />
        <activity
            android:name=".LoginActivity"
            android:parentActivityName=".ProfileActivity" />
        <activity android:name=".ProfileActivity" />
        <activity
            android:name=".RegisterActivity"
            android:parentActivityName=".ProfileActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".BookingsActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>